{
  "name": "gatsby-puppeteer-tests",
  "description": "An example of how to use Puppeteer to test Gatsby sites",
  "version": "0.1.0",
  "author": "Tim McMackin <timothymcmackin@yahoo.com>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "build:docker": "docker build -t gatsby-puppeteer-tests .",
    "develop:docker": "docker run --rm -it -p 8000:8000 gatsby-puppeteer-tests",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "start-server-and-test develop http://localhost:8000 'mocha ./tests/puppeteer-test.js'"
  },
  "dependencies": {
    "@gatsby-contrib/gatsby-plugin-elasticlunr-search": "2.4.2",
    "gatsby": "2.27.4",
    "gatsby-image": "2.6.0",
    "gatsby-plugin-manifest": "2.7.0",
    "gatsby-plugin-react-helmet": "3.5.0",
    "gatsby-plugin-sharp": "2.9.1",
    "gatsby-remark-images": "3.6.0",
    "gatsby-source-filesystem": "2.6.1",
    "gatsby-transformer-remark": "2.11.1",
    "gatsby-transformer-sharp": "2.7.0",
    "lodash": "4.17.20",
    "react": "17.0.1",
    "react-bootstrap": "1.4.0",
    "react-dd-menu": "2.0.2",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "remark": "13.0.0",
    "strip-markdown": "4.0.0",
    "typescript": "4.1.2"
  },
  "devDependencies": {
    "mocha": "8.2.1",
    "prettier": "2.2.1",
    "puppeteer": "5.5.0",
    "start-server-and-test": "1.11.6"
  },
  "resolutions": {
    "react": "17.0.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/timothymcmackin/gatsby-puppeteer-tests"
  },
  "bugs": {
    "url": "https://github.com/timothymcmackin/gatsby-puppeteer-tests/issues"
  }
}
